xml.rows do
  xml.page(@page)
  xml.total(@nbr_pages)
  xml.records(@count)
  @room_setups.each do |room_setup|
    xml.row({:id => room_setup.id}) do
      xml.cell(Room.find(room_setup.room_id).name)
      xml.cell(SetupType.find(room_setup.setup_type_id).name)
      xml.cell(room_setup.capacity)
    end
  end
end
