xml.people do
  @people.each do |p|
    xml.person do
      name = p.GetFullName()
      xml.name(name)
      p.email_addresses.each do |email_address|
        if (email_address.isdefault == true)
          xml.address(email_address.email)
        end
      end
      xml.key(p.survey_respondent.key)
    end
  end
end
