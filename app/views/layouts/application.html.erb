<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE9" >
    <title><%= @page_title or 'Convention Planner' %></title>
    <%=render 'layouts/common_styles' %>
    <%=render 'layouts/common_js' %>
    <%= yield(:cssheader) if content_for?(:cssheader) %>
    <%= yield(:jsheader) if content_for?(:jsheader) %>
    <style type="text/css">
        <%= yield(:styleheader) if content_for?(:styleheader) %>
    </style>

    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
</head>
<body>

<%=render 'layouts/common_page_top' %>

<%= content_for?(:content) ? yield(:content) : yield %>

</body>
</html>
