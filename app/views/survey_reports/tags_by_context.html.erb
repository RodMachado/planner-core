<h2>Participants with tags by context</h2>

<% form_tag('/survey_reports/tags_by_context', :method => 'post') do %>
<p>
   <%= label_tag :q_id, "Context: "%>
   <%= select_tag :q_id, options_for_select(@contexts, params[:q_id]) %>
   <%= submit_tag "Search" %>
</p>
<% end %>

<% if params[:q_id]  %>
<%= render :partial => "resplist", :object => @names %> 
<% end %>
