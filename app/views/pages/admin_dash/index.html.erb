<% content_for :submenu do %>
        <%= render_cell :context_menu, :display, :menu => {
            "submenu-users" => ('/pages/admin_dash/users' if allowed? :manage_users),
            "submenu-config" => ('/pages/admin_dash/configs' if allowed? :manage_config)
        }.keep_if{|k,v| v} %>
<% end %>

<% if @cellname %>
    <%= render_cell "admin/#{@cellname}", :display %>
<% else %>
    <%= render_cell "admin/admin_dash", :display %>
<% end %>
