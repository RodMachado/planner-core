<% content_for :submenu do %>
        <%= render_cell :context_menu, :display, :menu => {
            "manage-mailings" => '/pages/communications_dash/manage',
            "mailing-reports" => '/pages/communications_dash/report',
            "mail-templates"  => '/pages/communications_dash/templates'
        } %>
<% end %>

<% if @cellname %>
    <%= render_cell "communications/#{@cellname}", :display %>
<% else %>
    <%= render_cell "communications/communications_dash", :display %>
<% end %>
