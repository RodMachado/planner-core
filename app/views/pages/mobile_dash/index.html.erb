<% content_for :submenu do %>
        <%= render_cell :context_menu, :display, :menu => {
            (t "submenu-mobile-colors") => '/pages/mobile_dash/theme',
            (t "submenu-mobile-images") => '/pages/mobile_dash/images',
            (t "submenu-mobile-pages") => '/pages/mobile_dash/pages'
        } %>
<% end %>

<% if @cellname %>
    <%= render_cell "mobile/#{@cellname}", :display %>
<% end %>
