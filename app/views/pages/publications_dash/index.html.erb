<% content_for :submenu do %>
        <%= render_cell :context_menu, :display, :menu => {
            (t "submenu-print") => '/pages/publications_dash/print',
            (t "submenu-online") => '/pages/publications_dash/online'
        } %>
<% end %>

<% if @cellname %>
    <%= render_cell "publications/#{@cellname}", :display %>
<% else %>
    <%= render_cell "publications/operations_dash", :display %>
<% end %>
