<% content_for :submenu do %>
        <%= render_cell :context_menu, :display, :menu => {
            "participants" => '/pages/participants_dash/manage',
            "review-surveys" => '/pages/participants_dash/survey_review',
            "participants-import" => '/pages/participants_dash/import'
        } %>
<% end %>

<% if @cellname %>
    <%= render_cell "participants/#{@cellname}", :display %>
<% else %>
    <%= render_cell "participants/participant_dash", :display %>
<% end %>
