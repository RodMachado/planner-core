<br/>
<h1> Create a format</h1>
<!-- %= error_messages_for :format % -->
<%= form_for @format, :html => {:'accept-charset' => 'UTF-8'} do |f| -%>
	<p>
		<%= f.label :name, "Name " %> <br/>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :position, "Menu Order " %> <br/>
		<%= f.text_field :position %>
	</p>
	<p>
		<%= f.submit "Submit" %>
	</p>
<% end %>
