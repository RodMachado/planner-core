

<script type="text/template" id="mobile-pages-list-template">
    <div class="row">
        <div class="col-xs-1">
            <div id="mobile-pages-title-div"></div>
        </div>
        <div class="col-xs-11">
            <div id="mobile-pages-control-div"></div>
        </div>
    </div>
    <div class="row">
        <div id="mobile-pages-region-div" class="col-xs-12"></div>
    </div>
</script>


<script type="text/template" id="equipment-control-template">
    <button class="add-model-button btn-xs btn-primary pull-right">
        <%= t 'add-mobile-page' %>
    </button>
</script>

<script type="text/template" id="mobile-page-template">
    <td>
        <a href="#mobile-page-tab" data-toggle="tab" class="model-select-button">{{= title }}</a>
    </td>
    <td>
        {{= url }}
    </td>
    <td>
        {{= position }}
    </td>
    <td>
        <button class="model-edit-button btn btn-xs"><i class="glyphicon glyphicon-pencil"></i></button>
    </td>
    <td>
        <button class="model-delete-button btn btn-xs"><i class="glyphicon glyphicon-trash"></i></button>
    </td>
</script>
